      .flot-x-axis.flot-x1-axis.xAxis.x1Axis{:style => "position: absolute; top: 0px; left: 0px; bottom: 0px; right: 0px; display: block;"}
        .flot-tick-label.tickLabel{:style => "position: absolute; max-width: 98px; top: 213px; left: 89px; text-align: center;"} Sep 16
